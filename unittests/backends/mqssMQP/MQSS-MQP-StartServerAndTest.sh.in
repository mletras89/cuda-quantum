# Launch the fake server
./CrowRestServer &
# we'll need the process id to kill it
pid=$(echo "$!")
sleep 2
# Run the tests
./test-mqp-mqss
# Did they fail? 
testsPassed=$?
# kill the server
kill -INT $pid
# return success / failure
exit $testsPassed
